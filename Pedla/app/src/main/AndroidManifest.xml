<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pedla">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IfMerchant"></activity>
        <activity
            android:name=".Bookings"
            android:label="Bookings" />
        <activity
            android:name=".Stores"
            android:label="@string/text_stores_to_sidebar"></activity>
        <activity
            android:name=".OldCity"
            android:label="Old City" />
        <activity
            android:name=".GovardhanSagar"
            android:label="Govardhan Sagar" />
        <activity
            android:name=".SajjanGarh"
            android:label="Sajjan-Garh" />
        <activity
            android:name=".DoodhTalayi"
            android:label="Doodh Talayi" />
        <activity
            android:name=".RaniRoad"
            android:label="Rani-Road" />
        <activity
            android:name=".BadiLake"
            android:label="Badi-Lake" />
        <activity
            android:name=".AboutApp"
            android:label="About Pedla" />
        <activity
            android:name=".CovidRules"
            android:label="COVID-19 Rules" />
        <activity
            android:name=".BookingSuccessfull"
            android:label="Booking Successful" />
        <activity
            android:name=".Payment"
            android:label="@string/text_payment_to_orderreview"></activity>
        <activity
            android:name=".Cycles"
            android:label="@string/text_cycle_to_stores"></activity>
        <activity
            android:name=".OrderReview"
            android:label="@string/text_orderreview_toAccessories"></activity>
        <activity
            android:name=".Accessories"
            android:label="@string/text_accessories_to_cycles"></activity>
        <activity
            android:name=".SideBar"
            android:label="@string/title_activity_side_bar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Choice"
            android:label="Choice"
            android:theme="@style/AppTheme.RemoveActionBar" />
        <activity
            android:name=".Signup"
            android:label="Signup"
            android:theme="@style/AppTheme.RemoveActionBar" />
        <activity
            android:name=".Login"
            android:label="Login"
            android:theme="@style/AppTheme.RemoveActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.RemoveActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Preference"
            android:label="Settings"
            android:theme="@style/AppTheme" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>